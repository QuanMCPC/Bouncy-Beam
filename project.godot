[autoload]

Global="*res://Scene/Global.tscn"

[debug]

settings/stdout/print_fps=true

[display]

window/size/width=960
window/size/height=720

[gdnative]

singletons=[ "res://addons/godot-git-plugin/git_api.gdnlib" ]

[global]

size=false
theme=true

[gui]

